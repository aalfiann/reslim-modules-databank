		{
			"name": "Index with Keywords",
			"request": {
				"method": "GET",
				"header": [],
				"body": {
					"mode": "raw",
					"raw": ""
				},
				"url": {
					"raw": "http://localhost:1337/reSlim/src/api/data_bank/index/keywords/{yourusername}/{yourtoken}/{page}/{itemsperpage}/?query={query to search}&keywords={the keywords}&firstdate={yyyy-mm-dd}&lastdate={yyyy-mm-dd}",
					"protocol": "http",
					"host": [
						"localhost"
					],
					"port": "1337",
					"path": [
						"reSlim",
						"src",
						"api",
						"data_bank",
						"index",
